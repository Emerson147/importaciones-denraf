{
  "name": "sistema-master",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "build:prod": "ng build --configuration production --optimization --output-hashing=all",
    "build:analyze": "ng build --configuration production --stats-json && npx webpack-bundle-analyzer dist/sistema-master/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "migrate-images": "ts-node --esm scripts/migrate-images.ts",
    "migrate-base64-images": "ts-node --esm scripts/migrate-base64-images.ts",
    "analyze-storage": "ts-node --esm scripts/analyze-storage.ts"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.0",
  "dependencies": {
    "@angular/animations": "^21.0.1",
    "@angular/cdk": "^21.0.1",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/service-worker": "^21.0.0",
    "@spartan-ng/brain": "^0.0.1-alpha.567",
    "@supabase/supabase-js": "^2.89.0",
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/qrcode": "^1.5.6",
    "apexcharts": "^5.3.6",
    "clsx": "^2.1.1",
    "idb": "^8.0.3",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "ng-apexcharts": "^2.0.4",
    "postcss": "^8.5.6",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.3.0",
    "tw-animate-css": "^1.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "@spartan-ng/cli": "^0.0.1-alpha.567",
    "@types/node": "^25.0.3",
    "jsdom": "^27.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
